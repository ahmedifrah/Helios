<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9fc5f8"

    tools:context=".GoodResult">

    <Button
        android:id="@+id/button14"
        android:layout_width="172dp"
        android:layout_height="70dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="747dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="52dp"
        android:background="#ffffff"
        android:onClick="Back"
        android:text="Home"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="267dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="30dp"
        android:autoSizeTextType="uniform"
        android:text="You are Fine!"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="192dp"
        android:layout_height="185dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="186dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="371dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.723"
        app:srcCompat="@drawable/ic_mental_health__2_" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="311dp"
        android:layout_height="208dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="479dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="182dp"
        android:justificationMode="inter_word"
        android:text="Based on your readings, The machine learning has predicted that you are fine right now! But remember that prediction is not 100% accurate, so take care!"
        android:textColor="#000000"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button13"
        android:layout_width="447dp"
        android:layout_height="133dp"
        android:layout_marginBottom="612dp"
        android:background="@drawable/topbar"
        android:drawableLeft="@drawable/ic_done"
        android:paddingStart="25dp"
        android:paddingEnd="50dp"
        android:text="HELIOS"
        android:textAlignment="center"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>